{
    "connects":[
       {"name":"myConnect",
        "Addr":"amqp://admin:admin@10.100.100.154//dbees"}
    ],
    "channels":[
       {"name":"TestChannel",
        "connect":"myConnect",
        "qos_count":1,
        "qos_size":0},
       {"name":"errChannel",
        "connect":"myConnect",
        "qos_count":1,
        "qos_size":0},
       {"name":"dlxChannel",
        "connect":"myConnect",
        "qos_count":1,
        "qos_size":0}
    ],
    "exchanges":[
       {"name":"smsExchange",
        "channel":"TestChannel",
        "type":"direct",
        "durable":false,
        "auto_deleted":false,
        "internal":false,
        "no_wait":true,
         "ebind":[
           {"destination":"smsExchange","key":"major_Exchanges","no_wait":true}
         ],
        "args":{
            "alternate-exchange":"errServerExchange"
            }
        },
       {"name":"errServerExchange",
        "channel":"errChannel",
        "type":"fanout",
        "durable":false,
        "auto_deleted":false,
        "internal":true,
        "no_wait":false,
         "ebind":[
           {"destination":"errServerExchange","key":"error_Exchanges","no_wait":false}
         ]
        },
       {"name":"dlxExchange",
        "channel":"dlxChannel",
        "type":"fanout",
        "durable":false,
        "auto_deleted":false,
        "internal":true,
        "no_wait":false,
         "ebind":[
           {"destination":"dlxExchange","key":"backup_Exchanges","no_wait":false}
         ]
        }
    ],
    "queue":[
       {"name":"yf_manage_message",
        "channel":"TestChannel",
        "durable":true,
        "auto_delete":false,
        "exclusive":false,
        "no_wait":false,
        "qbind":[
            {"Key":"yf_manage_message", "exchange_name":"smsExchange", "no_wait":false}
        ],
        "args":{
            "x-message-ttl":100,
            "x-dead-letter-exchange":"dlxExchange",
            "x-dead-letter-routing-key":"dlxQueue"
        }},
       {"name":"errServerQueue",
        "channel":"errChannel",
        "durable":true,
        "auto_delete":false,
        "exclusive":false,
        "no_wait":false,
        "qbind":[
            {"Key":"errServerQueue", "exchange_name":"errServerExchange", "no_wait":false}
        ],
        "args":{
          "":""
        }},
       {"name":"dlxQueue",
        "channel":"dlxChannel",
        "durable":true,
        "auto_delete":false,
        "exclusive":false,
        "no_wait":false,
        "qbind":[
            {"Key":"dlxQueue", "exchange_name":"dlxExchange", "no_wait":false}
        ],
        "args":{
          "":""
        }}
    ],
    "pusher":[
       {"name":"myPusher",
        "channel":"TestChannel",
        "exchange":"smsExchange",
        "Key":"yf_manage_message",
        "mandatory":false,
        "immediate":false,
        "content_type":"text/plain",
        "delivery_mode":0}
    ],
    "popup":[
       {"name":"myPoper",
        "channel":"TestChannel",
        "q_name":"yf_manage_message",
        "consumer":"主交换机Consumer",
        "AutoACK":true,
        "exclusive":false,
        "no_local":false,
        "no_wait":false
       },
       {"name":"errPoper",
        "channel":"errChannel",
        "q_name":"errServerQueue",
        "consumer":"错误交换机Consumer",
        "AutoACK":true,
        "exclusive":false,
        "no_local":false,
        "no_wait":false
       },
       {"name":"dlxPoper",
        "channel":"dlxChannel",
        "q_name":"dlxQueue",
        "consumer":"备份交换机Consumer",
        "AutoACK":true,
        "exclusive":false,
        "no_local":false,
        "no_wait":false
       },
       {"name":"DBDataId",
        "channel":"TestChannel",
        "q_name":"yf_sms_send",
        "consumer":"DBDataIdConsumer",
        "AutoACK":true,
        "exclusive":false,
        "no_local":false,
        "no_wait":false
       }
    ]
}
